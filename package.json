{
  "name": "smk1085-ng2azureadb2c",
  "version": "1.0.0",
  "author": {
    "name": "Sven Maschek"
  },
  "description": "Package for the Azure AD B2C example for Angular2",
  "scripts": {
    "start": "concurrently \"npm run tsc:w\" \"npm run lite\" \"node api\\server.js \" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server --config=liteserver-app-config.json",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test-once": "tsc && karma start karma.conf.js --single-run",
    "typings": "typings",
    "postinstall": "typings install"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.0.0-rc.6",
    "@angular/compiler": "2.0.0-rc.6",
    "@angular/core": "2.0.0-rc.6",
    "@angular/forms": "2.0.0-rc.6",
    "@angular/http": "2.0.0-rc.6",
    "@angular/platform-browser": "2.0.0-rc.6",
    "@angular/platform-browser-dynamic": "2.0.0-rc.6",
    "@angular/router": "3.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.6",
    "angular2-uuid": "^1.1.0",
    "bootstrap": "4.0.0-alpha.4",
    "core-js": "^2.4.1",
    "es6-shim": "^0.35.1",
    "express": "^4.14.0",
    "jsonwebtoken": "^7.1.9",
    "reflect-metadata": "^0.1.3",
    "request": "^2.74.0",
    "rxjs": "5.0.0-beta.11",
    "systemjs": "0.19.38",
    "zone.js": "^0.6.17"
  },
  "devDependencies": {
    "canonical-path": "0.0.2",
    "concurrently": "^2.2.0",
    "http-server": "^0.9.0",
    "lite-server": "^2.2.2",
    "lodash": "^4.15.0",
    "tslint": "^3.15.1",
    "typescript": "^1.8.10",
    "typings": "^1.3.3",
    "jasmine-core": "~2.4.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-htmlfile-reporter": "^0.3.4",
    "karma-jasmine": "^1.0.2",
    "protractor": "^4.0.5",
    "rimraf": "^2.5.4"
  }
}
