{
    "name": "smk1085-ng2azureadb2c",
    "version": "1.0.0",
    "author": {
        "name": "Sven Maschek"
    },
    "description": "Package for the Azure AD B2C example for Angular2",
    "scripts": {
        "start": "concurrently \"npm run tsc:w\" \"npm run lite\" ",
        "tsc": "tsc",
        "tsc:w": "tsc -w",
        "lint": "tslint ./app/**/*.ts -t verbose",
        "lite": "lite-server --config=liteserver-app-config.json",
        "typings": "typings",
        "postinstall": "typings install"
    },
    "license": "MIT",
    "dependencies": {
        "angular2": "2.0.0-beta.17",
        "systemjs": "0.19.38",
        "es6-shim": "^0.35.1",
        "reflect-metadata": "0.1.2",
        "rxjs": "5.0.0-beta.6",
        "zone.js": "0.6.17",
        "bootstrap": "4.0.0-alpha.4"
    },
    "devDependencies": {
        "concurrently": "^2.2.0",
        "lite-server": "^2.2.2",
        "tslint": "^3.15.1",
        "typescript": "^1.8.10",
        "typings": "^1.3.3"
    }
}